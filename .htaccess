RewriteEngine on
RewriteRule ^1.x/(.*)$ 1.1/$1 [env=nocache:true]
RewriteRule ^2.x/(.*)$ 2.0/$1 [env=nocache:true]
RewriteRule ^3.x/(.*)$ 3.0/$1 [env=nocache:true]
RewriteRule ^4.x/(.*)$ 4.1/$1 [env=nocache:true]
RewriteRule ^latest/(.*)$ 4.1/$1 [env=nocache:true]

Header set Cache-Control no-cache env=REDIRECT_nocache
